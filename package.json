{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Yet another portfolio site",
  "scripts": {
    "removeDist": "rm -fr ./dist ",
    "start": "npm-run-all --parallel test:watch launch lint:watch",
    "build": "webpack --mode production",
    "launch": "removeDist & webpack-dev-server --hot --port=1235 --history-api-fallback --mode development",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "author": "Pavel Durov",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-bootstrap": "0.32.1",
    "react-burger-menu": "2.3.0",
    "react-dom": "16.2.0",
    "react-grid-gallery": "0.4.8",
    "react-localize-redux": "2.16.0",
    "react-redux": "5.0.7",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "cheerio": "0.22.0",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "1.1.2",
    "compression": "1.7.1",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "enzyme": "2.9.1",
    "eslint": "4.14.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "3.1.3",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "express": "4.16.2",
    "file-loader": "0.8.5",
    "img-loader": "2.0.0",
    "jsdom": "11.5.1",
    "mocha": "4.0.1",
    "nock": "8.2.2",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.4.0",
    "rimraf": "2.6.2",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webidl-conversions": "4.0.2",
    "webpack": "4.1.1",
